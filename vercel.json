{
  "version": 2,
  "buildCommand": "cd app/back && npm install && cd ../front && npm install && npm run build",
  "outputDirectory": "app/front/dist",
  "installCommand": "cd app/back && npm install && cd ../front && npm install",
  "framework": null,
  "functions": {
    "api/**/*.cjs": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/server"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

